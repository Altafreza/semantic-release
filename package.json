{
  "name": "semantic-release",
  "private": true,
  "type": "module",
  "version": "1.5.10",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start:dev": "vite --mode development",
    "start:qa": "vite --mode qa",
    "start:uat": "vite --mode uat",
    "start:prod": "vite --mode production",
    "build:dev": "vite build --mode development",
    "build:qa": "vite build --mode qa",
    "build:uat": "vite build --mode uat",
    "build:prod": "vite build --mode production",
    "test:e2e": "cypress run",
    "release": "semantic-release --no-ci",
    "release:beta": "semantic-release --no-ci --branches develop",
    "release:qa": "semantic-release --no-ci --branches qa",
    "release:uat": "semantic-release --no-ci --branches uat",
    "release:hotfix": "semantic-release --no-ci --branches hotfix/*",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test:watch": "cypress open"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "semantic-release": "^24.2.1",
    "vite": "^6.0.5"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "description": "A demo project to showcase semantic-release with proper branch versioning"
}
